{
  "name": "ai-baseball-cards",
  "version": "1.0.0",
  "description": "Baseball player stats viewer with AI-generated descriptions • React + Fastify + LangGraph + Prisma • TypeScript monorepo",
  "private": true,
  "scripts": {
    "dev": "docker-compose up",
    "dev:build": "docker-compose up --build",
    "dev:local": "pnpm --filter backend dev & pnpm --filter frontend dev",
    "build": "pnpm -r build",
    "lint": "pnpm -r lint",
    "format": "pnpm -r format",
    "clean": "pnpm -r clean && rm -rf node_modules",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:rebuild": "docker-compose down && docker-compose up --build -d"
  },
  "keywords": [
    "baseball",
    "ai",
    "react",
    "fastify",
    "langgraph",
    "prisma",
    "typescript",
    "monorepo"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  }
}
