{
  "name": "@ai-baseball-cards/backend",
  "version": "1.0.0",
  "description": "Fastify backend API for AI Baseball Cards",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "biome lint src/",
    "format": "biome format --write src/",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "seed": "tsx src/utils/seed.ts",
    "clean": "rm -rf dist node_modules"
  },
  "keywords": ["fastify", "api", "baseball", "prisma"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/cors": "^10.0.1",
    "@fastify/swagger": "^9.1.0",
    "@fastify/swagger-ui": "^5.1.0",
    "@prisma/client": "^5.22.0",
    "dotenv": "^16.4.7",
    "fastify": "^5.2.0",
    "zod": "^3.24.1",
    "zod-to-json-schema": "^3.23.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.10.2",
    "prisma": "^5.22.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
